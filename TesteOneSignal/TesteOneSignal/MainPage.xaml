<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TesteOneSignal"
             x:Class="TesteOneSignal.MainPage">

    <StackLayout>
        <!-- Place new controls here -->
        <Label x:Name="lblIdUser" />
        <Label Text="Teste de Notificação" HorizontalOptions="Center"/>
        <Entry Placeholder="Digite sua notificação..." x:Name="txtNotificacao"/>
        <DatePicker x:Name="dataPicker" Format="dd-MM-yyyy"/>
        <TimePicker x:Name="timePicker" Format="HH:mm"/>
        <Button Text="Enviar Notificação" x:Name="btEnviar" Clicked="btClicado"/>
        <Label x:Name="lblAviso"/>
    </StackLayout>

</ContentPage>
